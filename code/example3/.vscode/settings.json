{
	"mochaExplorer.files": "src/tests/**/*.test.ts",
	"mochaExplorer.watch": "./src/tests/**/*.test.ts",
	"mochaExplorer.nodeArgv": ["-r", "ts-node/register"],
	"mochaExplorer.env": {
		"TS_NODE_PROJECT": "./tsconfig.json"
